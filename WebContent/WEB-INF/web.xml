    <servlet-name>NouveauCandidat</servlet-name>
    <url-pattern>/nouveaucandidat</url-pattern>
  </servlet-mapping>
  <description>Fiche Etudiant</description>
  <servlet>
    <servlet-name>FicheEtudiant</servlet-name>

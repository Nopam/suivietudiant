    <servlet-class>com.suivicandidature.servlets.ServletUn</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletUn</servlet-name>
    <url-pattern>/un</url-pattern>
  </servlet-mapping>
  <description>Servlet Accueil</description>

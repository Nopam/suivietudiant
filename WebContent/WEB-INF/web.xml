    <servlet-class>com.suivicandidature.servlets.Accueil</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Accueil</servlet-name>
    <url-pattern>/accueil</url-pattern>
  </servlet-mapping>
  <description>Servlet Candidatures</description>
  <servlet>
    <servlet-name>NouveauCandidat</servlet-name>
    <servlet-class>com.suivicandidature.servlets.NouveauCandidat</servlet-class>
  </servlet>
